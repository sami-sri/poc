      <sub id="script">
        <sub id="script" style="white-space:pre;">## Test Runs for current Iteration
#set($quo = &#39;&quot;&#39;)
#set($projectId = $page.getReference().projectId())
#set($currentIterationQuery = &quot;project.id:$projectId AND template.id:iteration AND status:inprogress&quot;)
#set($currentIterations = $transaction.plans().search().query(&quot;$currentIterationQuery&quot;))
#if($currentIterations.size() &gt; 1)
	$widgetContext.renderWarning(&quot;More than one Iteration is currently &lt;b&gt;In Progress&lt;/b&gt;.&quot;)
#elseif($currentIterations.iterator().hasNext())
	#set($currentIteration = $currentIterations.iterator().next())
	#set($testRunIDsForIteration = &quot;&quot;)
	#set($testRunsForIterationQuery = &quot;project.id:$projectId AND planScope.KEY:($currentIteration.getReference().id())&quot;)
	#set($testRunsForIteration = $transaction.testRuns().search().query(&quot;$testRunsForIterationQuery&quot;))
	#foreach($testRun in $testRunsForIteration)
		#set($testRunIDsForIteration = &quot;$testRunIDsForIteration$quo$testRun.getReference().id()$quo &quot;)
	#end
	#if(!$testRunIDsForIteration.equals(&quot;&quot;))
		#set($query = &quot;id:($testRunIDsForIteration.trim())&quot;)
	#else
		#set($query = &quot;NOT *:*&quot;)
	#end
	&lt;div class=&quot;polarion-rp-widget-part&quot; data-widget=&quot;com.polarion.table&quot;&gt;
		&lt;span class=&quot;polarion-rp-widget-parameters&quot;&gt;
			&lt;sub id=&quot;dataSet&quot;&gt;
				&lt;sub id=&quot;prototype&quot;&gt;TestRun&lt;/sub&gt;
				&lt;sub id=&quot;queryType&quot;&gt;luceneVelocity&lt;/sub&gt;
				&lt;sub id=&quot;luceneQuery&quot;&gt;$query&lt;/sub&gt;
				&lt;sub id=&quot;children&quot;&gt;
					&lt;sub id=&quot;columns&quot;&gt;
						&lt;sub&gt;
							&lt;sub id=&quot;id&quot;&gt;id&lt;/sub&gt;
						&lt;/sub&gt;
						&lt;sub&gt;
							&lt;sub id=&quot;id&quot;&gt;status&lt;/sub&gt;
						&lt;/sub&gt;
						&lt;sub&gt;
							&lt;sub id=&quot;id&quot;&gt;template&lt;/sub&gt;
						&lt;/sub&gt;
						&lt;sub&gt;
							&lt;sub id=&quot;id&quot;&gt;finishedOn&lt;/sub&gt;
						&lt;/sub&gt;
						&lt;sub&gt;
