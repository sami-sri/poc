<pre id="polarion-rp-page-script">###############################################################################
##
## Page Script
##
## com.polarion.alm.shared.api.transaction.ReadOnlyTransaction $transaction
## com.polarion.alm.shared.api.model.rp.RichPage $page
##

#set($query = &quot;project.id:($page.getReference().projectId()) AND type:userstory&quot;)
#if(!$pageParameters.query.value().isEmpty())
	#set($query = &quot;$query AND $pageParameters.query.value()&quot;)
#end
#if($pageParameters.plannedIn.singleValue())
	#set($query = &quot;$query AND PLAN:($pageParameters.plannedIn.singleValue().getModelObject().getReference().toPath())&quot;)
#end
#set($userStories = $transaction.workItems().search().query(&quot;$query&quot;))
#set($coveredIds = &quot;&quot;)
#set($uncoveredIds = &quot;&quot;)
#foreach($userStory in $userStories)
	#set($isCovered = false)
	#foreach($linkedWi in $userStory.fields().linkedWorkItems().back())
		#if($linkedWi.fields().role().optionId().equals(&quot;verifies&quot;))
			#if($linkedWi.fields().workItem().get().fields().type().get().id().equals(&quot;testcase&quot;))
				#set($isCovered = true)
			#end
		#end
	#end
	#if($isCovered)
		#set($coveredIds = &quot;$!coveredIds $userStory.getReference().id()&quot;)
	#else
		#set($uncoveredIds = &quot;$!uncoveredIds $userStory.getReference().id()&quot;)
	#end
#end

$!pageContext.put(&quot;coveredIds&quot;, $coveredIds)
$!pageContext.put(&quot;uncoveredIds&quot;, $uncoveredIds)

Covered IDs: $coveredIds &lt;br/&gt;
Uncovered IDs: $uncoveredIds
</pre>]]></field>
